{
	"name": "align-by-token",
	"displayName": "Align by Token",
	"description": "Align selected text vertically by a specified token",
	"version": "0.0.1",
	"publisher": "Addison Elliott",
	"repository": {
		"type": "git",
		"url": "https://github.com/addisonElliott/align-by-token.git"
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:extension.alignByToken"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.alignByToken",
				"title": "Align by Token"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"prettier": "^1.19.1",
		"tslint": "^5.20.0",
		"tslint-plugin-prettier": "^2.0.1",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
